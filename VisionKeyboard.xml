<Keyboard>
    <Name>Vision Keyboard</Name>
    <!-- <ShowOutputPanel>True</ShowOutputPanel> -->
    <SymbolMargin>15</SymbolMargin>
    <BorderThickness>1,1,1,1</BorderThickness>
    <Grid>
        <Rows>9</Rows>
        <Cols>11</Cols>
    </Grid>
    <Content>
		<DynamicKey Row="0" Col="0"><Action>PreviousSuggestions</Action><Label>Previous</Label><Symbol>SuggestionLeftIcon</Symbol></DynamicKey>
		<SuggestionRow Width="9" />
		<DynamicKey Row="0"><Action>NextSuggestions</Action><Label>Next</Label><Symbol>SuggestionRightIcon</Symbol></DynamicKey>
		<DynamicKey Row="1" Col="0" Height="3" ><Action>AddToDictionary</Action><Label>Add to Dictionary</Label><Symbol>AddToDictionaryIcon</Symbol></DynamicKey>
		<DynamicKey Row="1" Col="1" Height="3" ><Action>ClearScratchpad</Action><Label>Clear</Label><Symbol>ClearIcon</Symbol></DynamicKey>
		<Scratchpad Width="7" Height="3" />
		<DynamicKey Row="1" Col="9" Height="3"><Action>Speak</Action><Label>Speak</Label><Symbol>SpeakIcon</Symbol></DynamicKey>
		<!-- <DynamicKey><Action>CopyAllFromScratchpad</Action><Label>Copy</Label><Symbol>CopyAllIcon</Symbol></DynamicKey> -->
		<DynamicKey Row="1" Col="10" Height="3"><Action>Sleep</Action><Label>Sleep</Label><Symbol>SleepIcon</Symbol></DynamicKey>
		
        <DynamicKey Row="4" Col="10">
			<Symbol>MoreIcon</Symbol>
		</DynamicKey>
		<DynamicKey Row="4" Col="0">
            <ShiftDownLabel>!</ShiftDownLabel>
            <Label>1</Label>
            <Text>1</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>@</ShiftDownLabel>
            <Label>2</Label>
            <Text>2</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>#</ShiftDownLabel>
            <Label>3</Label>
            <Text>3</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>$</ShiftDownLabel>
            <Label>4</Label>
            <Text>4</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>%</ShiftDownLabel>
            <Label>5</Label>
            <Text>5</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>^</ShiftDownLabel>
            <Label>6</Label>
            <Text>6</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>&#038;</ShiftDownLabel>
            <Label>7</Label>
            <Text>7</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>*</ShiftDownLabel>
            <Label>8</Label>
            <Text>8</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>(</ShiftDownLabel>
            <Label>9</Label>
            <Text>9</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>)</ShiftDownLabel>
            <Label>0</Label>
            <Text>0</Text>
        </DynamicKey>
       <DynamicKey Row="5" Col="0">
            <ShiftDownLabel>Q</ShiftDownLabel>
            <Label>q</Label>
            <Text>q</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>W</ShiftDownLabel>
            <Label>w</Label>
            <Text>w</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>E</ShiftDownLabel>
            <Label>e</Label>
            <Text>e</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>R</ShiftDownLabel>
            <Label>r</Label>
            <Text>r</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>T</ShiftDownLabel>
            <Label>t</Label>
            <Text>t</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>Y</ShiftDownLabel>
            <Label>y</Label>
            <Text>y</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>U</ShiftDownLabel>
            <Label>u</Label>
            <Text>u</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>I</ShiftDownLabel>
            <Label>i</Label>
            <Text>i</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>O</ShiftDownLabel>
            <Label>o</Label>
            <Text>o</Text>
        </DynamicKey> 
        <DynamicKey>
            <ShiftDownLabel>P</ShiftDownLabel>
            <Label>p</Label>
            <Text>p</Text>
        </DynamicKey>
		<DynamicKey Row="5" Col="10">
            <ShiftDownLabel>?</ShiftDownLabel>
	        <Label>/</Label>
	        <Text>/</Text>
        </DynamicKey> 
        <DynamicKey Row="6" Col="0">
            <ShiftDownLabel>A</ShiftDownLabel>
            <Label>a</Label>
            <Text>a</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>S</ShiftDownLabel>
            <Label>s</Label>
            <Text>s</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>D</ShiftDownLabel>
            <Label>d</Label>
            <Text>d</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>F</ShiftDownLabel>
            <Label>f</Label>
            <Text>f</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>G</ShiftDownLabel>
            <Label>g</Label>
            <Text>g</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>H</ShiftDownLabel>
            <Label>h</Label>
            <Text>h</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>J</ShiftDownLabel>
            <Label>j</Label>
            <Text>j</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>K</ShiftDownLabel>
            <Label>k</Label>
            <Text>k</Text>
        </DynamicKey>
        <DynamicKey>
            <ShiftDownLabel>L</ShiftDownLabel>
            <Label>l</Label>
            <Text>l</Text>
        </DynamicKey>
	    <DynamicKey Row="7" Col="0">
            <ShiftDownLabel>Z</ShiftDownLabel>
            <Label>z</Label>
            <Text>z</Text>
        </DynamicKey>
        <DynamicKey Row="7" Col="1">
            <ShiftDownLabel>X</ShiftDownLabel>
            <Label>x</Label>
            <Text>x</Text>
        </DynamicKey>
        <DynamicKey Row="7" Col="2">
            <ShiftDownLabel>C</ShiftDownLabel>
            <Label>c</Label>
            <Text>c</Text>
        </DynamicKey>
        <DynamicKey Row="7" Col="3">
            <ShiftDownLabel>V</ShiftDownLabel>
            <Label>v</Label>
            <Text>v</Text>
        </DynamicKey>
        <DynamicKey Row="7" Col="4">
            <ShiftDownLabel>B</ShiftDownLabel>
            <Label>b</Label>
            <Text>b</Text>
        </DynamicKey>
        <DynamicKey Row="7" Col="5">
            <ShiftDownLabel>N</ShiftDownLabel>
            <Label>n</Label>
            <Text>n</Text>
        </DynamicKey>
        <DynamicKey Row="7" Col="6">
            <ShiftDownLabel>M</ShiftDownLabel>
            <Label>m</Label>
            <Text>m</Text>
        </DynamicKey>
        <DynamicKey Row="7" Col="9">
	        <Action>BackOne</Action>
	        <Symbol>BackOneIcon</Symbol>
        </DynamicKey>
        <DynamicKey Row="7" Col="10">
	        <Action>BackMany</Action>
	        <Symbol>BackManyIcon</Symbol>
        </DynamicKey> 
        <DynamicKey Row="8" Col="0">
			<Label>SendSMS</Label>
				<Plugin>
					<Name>ExternalProgram</Name>
					<Method>RUN</Method>
						<Argument>
							<Name>command</Name>
							<Value>"C:\Users\desktop\SendSMS.bat"</Value>
						</Argument>
						<Argument>
							<Name>parameters</Name>
							<Value>$scratchpadText</Value>
						</Argument>
					</Plugin>
		</DynamicKey>
		<DynamicKey Row="8" Col="1" Width="2">
			<Action>LeftShift</Action>
			<Label>Shift</Label>
			<Symbol>ShiftIcon</Symbol>
		</DynamicKey>
		<DynamicKey Row="8" Col="3">
			<Symbol>NumericAndSymbolsIcon</Symbol>
			<ChangeKeyboard BackReturnsHere="True">Full</ChangeKeyboard>
		</DynamicKey>
        <DynamicKey Row="7" Col="8">
	        <ShiftDownLabel>&lt;</ShiftDownLabel>
            <Label>,</Label>
            <Text>,</Text>
        </DynamicKey>
	    <DynamicKey Row="8" Col="4" Width="2">
            <Symbol>SpaceIcon</Symbol>
            <Text>&#32;</Text>
	    </DynamicKey>
        <DynamicKey Row="7" Col="7">
            <ShiftDownLabel>&gt;</ShiftDownLabel>
            <Label>.</Label>
            <Text>.</Text>
        </DynamicKey>
		<DynamicKey Row="6" Col="9">
             <ShiftDownLabel>"</ShiftDownLabel>
            <Label>'</Label>
            <Text>'</Text>
	    </DynamicKey>
        <DynamicKey Row="6" Col="10">
            <ShiftDownLabel>|</ShiftDownLabel>
	        <Label>\</Label>
	        <Text>\</Text>
        </DynamicKey> 
        <DynamicKey Row="8" Col="9">
            <Symbol>MenuIcon</Symbol>
            <Action>MenuKeyboard</Action>
        </DynamicKey>
        <DynamicKey Row="8" Col="10">
            <Symbol>EnterIcon</Symbol>
            <Text>&#13;</Text>
        </DynamicKey>
    </Content>
</Keyboard>
